<g transform="translate(1000 1060)">
  <defs>
    <linearGradient id="aspectgradient">
      <stop offset="0%" stop-color="#FFFFF0"></stop>
      <stop offset="100%" stop-color="#BFBDB1"></stop>
    </linearGradient>
  </defs>
  <g style="filter: blur(5px);">
    {{#each planets length=8}}
      <circle cx="{{int16 x}}" cy="{{int16 y}}" r="11" fill="white"></circle>
    {{/each}}
  </g>
  {{#each aspects length=8}}
    <line
      x1="{{int16 x1}}"
      y1="{{int16 y1}}"
      x2="{{int16 x2}}"
      y2="{{int16 y2}}"
      stroke="url(#aspectgradient)"
      stroke-width="10"
      stroke-linecap="round"
    ></line>
  {{/each}}
</g>